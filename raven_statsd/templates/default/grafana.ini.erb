[paths]
data = <%=@data_dir%>

[server]
root_url = <%=@root_url%>

[security]
admin_user = <%=@admin_username%>
admin_password = <%=@admin_password%>

[auth.google]
enabled = true
client_id = <%=@google_client_id%>
client_secret = <%=@google_client_secret%>
allowed_domains = <%=@google_domain%>
allow_sign_up = true
